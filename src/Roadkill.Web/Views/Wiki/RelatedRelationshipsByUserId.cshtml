
@model IEnumerable<RelViewModel>

@if (Model.Count() > 0)
{
    <h2>Projects this user is associated with:</h2>
    <ul class="list-group">
        @foreach (RelViewModel pageModel in Model)
        {
            <li class="list-group-item">
                @if (pageModel.relTypeID == 3)
                {<span class="relationship-icon fa fa-eye"></span><span class="relationship-text">Watching</span>}
                @if (pageModel.relTypeID == 4)
                {<span class="relationship-icon fa fa-pencil"></span><span class="relationship-text">Contributing to</span>}

                <a href="/wiki/@pageModel.pageID">@pageModel.projectname</a>
            </li>
        }
    </ul>
}


