@model IEnumerable<RelViewModel>

@if (Model.Count() > 0)
{
  
        <div id="relationships" class="container">
            @foreach (RelViewModel pageModel in Model)
            {
                <div class="relationship">
                    <div class="row">
                        <div class="col-sm-2">
                            @if (pageModel.relTypeID == 1)
                            { <span class="relationship-icon glyphicon glyphicon-thumbs-up"></span><span class="relationship-text">Liked</span> }
                            @if (pageModel.relTypeID == 2)
                            { <span class="relationship-icon glyphicon glyphicon-plus-sign"></span><span class="relationship-text">Joined</span> }
                        </div>
                        <div class="col-sm-3">
                            @pageModel.usertext - @pageModel.orgtext
                        </div>
                        <div class="col-sm-7">
                            @pageModel.reltext
                        </div>
                    </div>
                </div>
            }
        </div>

}


